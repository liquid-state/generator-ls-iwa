{
  "name": "generator-ls-iwa",
  "version": "1.3.2",
  "description": "A Yeoman generator for creating Liquid State Integrated Web Apps (IWAs)",
  "homepage": "https://www.liquid-state.com",
  "author": {
    "name": "Liquid State",
    "email": "dev@liquid-state.com",
    "url": "https://www.liquid-state.com"
  },
  "files": [
    "generators",
    "utils"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^4.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^4.2.1",
    "prettier": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-react": "^7.0.0",
    "chalk": "^2.1.0",
    "yeoman-generator": "^2.0.5",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "liquid-state/generator-ls-iwa",
  "license": "MIT"
}
