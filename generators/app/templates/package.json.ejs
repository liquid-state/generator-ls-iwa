{
  "name": "<%= name %>",
  "private": <%= private %>,
  "version": "0.1.0",
  "description": "<%= description %>",
  "homepage_comment": "Liquid State IWAs must be able to load from file system, therefore homepage must be relative.",
  "homepage": "./",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },<% if (gitUrl && gitUrl.length > 0) { %>
  "repository": {
    "type": "git",
    "url": "<%= gitUrl %>"
  },<% } %>
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .js,.jsx",
    "lint:fix": "eslint ./src --fix --ext .js,.jsx",
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@liquid-state/iwa-core": "^0.1.4",
    "@liquid-state/iwa-router": "^0.1.3",
    "@liquid-state/ui-kit": "^0.1.1",
    "antd": "^3.2.0",
    "history": "^4.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.5",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^0.15.0-rc.13",
    "lint-staged": "^7.0.0",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.4.1"
  }
}
