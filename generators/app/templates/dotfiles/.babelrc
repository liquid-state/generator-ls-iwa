{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false
            }
        ],
        [
            "@babel/preset-react"
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-class-properties"
        ],
        [
            "import",
            {
                "libraryName": "antd",
                "style": true
            }
        ],
        [
          "react-hot-loader/babel"
        ]
    ]
}
